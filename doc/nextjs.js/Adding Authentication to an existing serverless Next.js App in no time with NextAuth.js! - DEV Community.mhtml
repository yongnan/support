  <span class=3D"na">providers</span><span class=3D"p">:</span> <span class=
=3D"p">[</span>
    <span class=3D"nx">Providers</span><span class=3D"p">.</span><span clas=
s=3D"nx">Google</span><span class=3D"p">({</span>
      <span class=3D"na">clientId</span><span class=3D"p">:</span> <span cl=
ass=3D"nx">process</span><span class=3D"p">.</span><span class=3D"nx">env</=
